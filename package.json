{
	"name": "cf-multiworker-boilerplate",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*",
		"durable-objects/*"
	],
	"scripts": {
		"build": "turbo build",
		"dev": "turbo dev --ui tui",
		"lint": "turbo lint",
		"typecheck": "turbo typecheck",
		"predeploy": "bun --env-file ./.env.local ./scripts/predeploy.ts",
		"deploy": "bun --env-file ./.env.local turbo deploy"
	},
	"devDependencies": {
		"@turbo/gen": "^2.5.8",
		"@types/bun": "^1.2.23",
		"jsonc-parser": "^3.3.1",
		"turbo": "^2.5.8"
	},
	"packageManager": "bun@1.2.23",
	"dependencies": {
		"uuid": "^13.0.0"
	}
}
