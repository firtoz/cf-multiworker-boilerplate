{
	"include": ["./workers/**/*.ts", "./worker-configuration.d.ts"],
	"compilerOptions": {
		"composite": true,
		"strict": true,
		"lib": ["ES2022"],
		"target": "ES2022",
		"module": "ES2022",
		"moduleResolution": "bundler",
		// "baseUrl": ".",
		// "rootDirs": ["."],
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"checkJs": true,
		"verbatimModuleSyntax": true,
		"skipLibCheck": true,
		"noEmit": true,
		"types": []
	}
}
