{
	"name": "processor-do",
	"private": true,
	"type": "module",
	"scripts": {
		"cf-typegen": "bunx wrangler types && bun run post-typegen",
		"post-typegen": "bun ../../scripts/post-typegen.ts",
		"typecheck": "tsc --noEmit -p ./tsconfig.json",
		"lint": "biome check --write .",
		"lint:format": "biome format --write .",
		"deploy": "bunx wrangler deploy"
	},
	"packageManager": "bun@1.3.0",
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@types/node": "catalog:",
		"typescript": "catalog:",
		"wrangler": "catalog:"
	},
	"dependencies": {
		"@firtoz/hono-fetcher": "catalog:",
		"@hono/zod-validator": "catalog:",
		"do-common": "workspace:*",
		"hono": "catalog:",
		"zod": "catalog:"
	}
}
