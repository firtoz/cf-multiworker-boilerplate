{
	"name": "cf-web-app",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"typegen": "bun run cf-typegen && bun run rr-typegen",
		"cf-typegen": "bun --cwd ../../packages/scripts cf-typegen $(pwd)",
		"rr-typegen": "react-router typegen",
		"deploy": "bunx wrangler deploy",
		"dev": "react-router dev",
		"preview": "bun run build && vite preview",
		"typecheck": "tsc --noEmit -p ./tsconfig.json",
		"lint": "biome check --write .",
		"lint:format": "biome format --write ."
	},
	"dependencies": {
		"@firtoz/hono-fetcher": "catalog:",
		"@firtoz/maybe-error": "^1.5.0",
		"@firtoz/router-toolkit": "^5.0.0",
		"clsx": "^2.1.1",
		"example-do": "workspace:*",
		"hono": "catalog:",
		"isbot": "^5.1.31",
		"jotai": "^2.15.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-router": "^7.9.4",
		"tailwind-merge": "^3.3.1",
		"uuid": "catalog:",
		"zod": "catalog:",
		"zod-form-data": "^3.0.1",
		"zod-validation-error": "^4.0.2"
	},
	"devDependencies": {
		"@biomejs/biome": "catalog:",
		"@cloudflare/vite-plugin": "^1.13.13",
		"@react-router/dev": "^7.9.4",
		"@tailwindcss/vite": "^4.1.14",
		"@types/json-diff": "^1.0.3",
		"@types/node": "catalog:",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"image-size": "^2.0.2",
		"rollup-plugin-visualizer": "^6.0.5",
		"source-map-explorer": "^2.5.3",
		"tailwindcss": "^4.1.14",
		"typescript": "catalog:",
		"vite": "^7.1.10",
		"vite-imagetools": "^9.0.0",
		"vite-plugin-devtools-json": "^1.0.0",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "catalog:"
	},
	"packageManager": "bun@1.3.0"
}
