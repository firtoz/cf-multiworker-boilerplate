		"json-diff": "^1.0.6",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router": "^7.9.3",
		"uuid": "^13.0.0",
		"zod": "^4.1.11",
		"zod-form-data": "^3.0.1",
		"zod-validation-error": "^4.0.2"
