		"json-diff": "^1.0.6",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router": "^7.8.0",
		"uuid": "^11.1.0",
		"zod": "^4.0.16",
		"zod-form-data": "^3.0.0",
		"zod-validation-error": "^4.0.1"
	},
	"devDependencies": {
