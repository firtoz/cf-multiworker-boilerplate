{
	"name": "cf-web-app",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"pre-typegen": "bun ../../scripts/pre-typegen.ts",
		"cf-typegen": "bunx wrangler types -c wrangler.temp.jsonc",
