{
	"name": "cf-web-app",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"cf-typegen": "bunx wrangler types -c wrangler.jsonc && bun run post-typegen",
		"post-typegen": "bun ../../scripts/post-typegen.ts",
