{
	"$schema": "https://turborepo.com/schema.json",
	"globalDependencies": [".env", "tsconfig.json"],
	"ui": "tui",
	"tasks": {
		"build": {
			"outputLogs": "new-only"
		},
		"typecheck": {},
		"typegen": {
			"outputLogs": "new-only"
		},
		"cf-typegen": {
			"outputLogs": "new-only"
		},
		"post-typegen": {
			"outputLogs": "new-only"
		},
		"rr-typegen": {
			"outputLogs": "new-only"
		},
		"dev": {
			"cache": false,
			"persistent": true,
			"outputLogs": "new-only"
		},
		"lint": {
			"inputs": ["biome.json"]
		},
		"deploy": {
			"outputLogs": "new-only"
		}
	},
	"globalEnv": ["CLOUDFLARE_API_TOKEN", "CLOUDFLARE_ACCOUNT_ID"]
}
