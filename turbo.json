{
	"$schema": "./node_modules/turbo/schema.json",
	"globalDependencies": [".env", "tsconfig.base.json"],
	"ui": "tui",
	"tasks": {
		"build": {
			"outputLogs": "new-only"
		},
		"typecheck": {},
		"typegen": {
			"outputLogs": "new-only"
		},
		"cf-typegen": {
			"outputLogs": "new-only"
		},
		"rr-typegen": {
			"outputLogs": "new-only"
		},
		"dev": {
			"cache": false,
			"persistent": true,
			"outputLogs": "new-only"
		},
		"lint": {},
		"pre-deploy": {
			"outputLogs": "new-only"
		},
		"deploy": {
			"cache": false,
			"outputLogs": "new-only"
		},
		"clean": {
			"cache": false
		}
	},
	"globalEnv": ["CLOUDFLARE_API_TOKEN", "CLOUDFLARE_ACCOUNT_ID", "SESSION_SECRET"]
}
