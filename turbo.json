{
	"$schema": "https://turborepo.com/schema.json",
	"globalDependencies": [".env", "tsconfig.json"],
	"ui": "tui",
	"tasks": {
		"build": {
			"outputLogs": "new-only"
		},
		"typecheck": {
			"outputLogs": "new-only"
		},
		"cf-typegen": {
			"outputLogs": "new-only"
		},
		"post-typegen": {
			"outputLogs": "new-only"
		},
		"rr-typegen": {
			"outputLogs": "new-only"
		},
		"dev": {
			"cache": false,
			"persistent": true,
			"outputLogs": "new-only"
		},
		"lint": {
			"outputLogs": "new-only"
		}
	}
}
